{
  "name": "course-aggregator",
  "version": "1.0.0",
  "description": "Horizontal Repository/Aggregator for Open Courses with React Front-end and Spark ML",
  "main": "backend/server.js",
  "scripts": {
    "start": "node backend/server.js",
    "dev": "nodemon backend/server.js",
    "client": "cd frontend && npm start",
    "install-all": "npm install && cd frontend && npm install",
    "spark": "spark-submit spark/ml_pipeline.py",
    "seed": "node backend/scripts/seedData.js"
  },
  "keywords": ["courses", "aggregator", "spark", "react", "ml"],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "mongoose": "^8.0.3",
    "axios": "^1.6.2",
    "node-cron": "^3.0.3",
    "cheerio": "^1.0.0-rc.12"
  },
  "devDependencies": {
    "nodemon": "^3.0.2"
  }
}

